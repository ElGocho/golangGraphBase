# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  ping: Boolean!
}

type Mutation {
  ping2: Boolean!

  register(input:RegisterInput!):User!
}


type User {
  id: ID!
  name: String!
  email: String!
  identification: String!
  image: String!
  password: String!
  role_id: Int!
}

input RegisterInput{
  user: UserInput!
}

input UserInput{
  name: String!
  email: String!
  identification: String!
  image: String!
  password: String!
  role_id: Int!
}
